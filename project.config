[project]
	description = Security Manager
[access]
	inheritFrom = All-Projects
[access "refs/for/refs/heads/*"]
	push = group co-securitymgr-committers
[access "refs/heads/*"]
	create = group co-securitymgr-committers
	forgeAuthor = group co-securitymgr-committers
	label-Code-Review = -2..+2 group co-securitymgr-committers
	submit = group co-securitymgr-committers
	editTopicName = group co-securitymgr-committers
	abandon = group co-securitymgr-committers
	removeReviewer = group co-securitymgr-committers
[access "refs/meta/config"]
	read = group co-securitymgr-committers
[access "refs/tags/*"]
	pushTag = group co-securitymgr-committers
	pushSignedTag = group co-securitymgr-committers
[access "refs/for/*"]
	pushMerge = group co-securitymgr-committers
[access "refs/for/refs/heads/feature/*"]
	pushMerge = group Non-Interactive Users
	pushMerge = group co-securitymgr-committers
[access "refs/*"]
	read = group GitHub Replication
